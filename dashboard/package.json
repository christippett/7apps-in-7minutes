{
  "name": "7apps-google-cloud",
  "version": "0.0.1",
  "description": "7-Apps in 7-Minutes",
  "main": "static/main.js",
  "scripts": {
    "bundle": "rollup -c",
    "watch": "rollup -c -w -m inline",
    "python": "(cd src && poetry run python main.py)",
    "start": "concurrently \"npm run watch\" \"npm run python\""
  },
  "author": "Chris Tippett <chris.tippett@servian.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "d3": "^5.16.0",
    "d3-path": "^2.0.0",
    "d3-scale": "^3.2.2",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-timer": "^2.0.0",
    "d3-transition": "^2.0.0",
    "webfontloader": "^1.6.28"
  }
}
